{
  "name": "gatsby-theme-maximeheckel",
  "description": "Gatsby theme for maximeheckel.com and other consumer gatsby projects",
  "version": "1.1.0",
  "main": "index.js",
  "types": "dist/index.d.ts",
  "author": "Maxime Heckel <hello@maximeheckel.com>",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint --ext=jsx,ts,tsx src",
    "semantic-release": "semantic-release",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/core": "^10.0.6",
    "@emotion/styled": "^10.0.6",
    "@sindresorhus/slugify": "^0.11.0",
    "focus-trap-react": "^6.0.0",
    "framer-motion": "3.1.1",
    "gatsby": "2.23.4",
    "gatsby-image": "2.4.7",
    "gatsby-plugin-emotion": "4.1.22",
    "gatsby-plugin-google-analytics": "^2.3.4",
    "gatsby-plugin-mdx": "^1.2.6",
    "gatsby-plugin-page-creator": "^2.1.5",
    "gatsby-plugin-react-helmet": "3.3.4",
    "gatsby-plugin-sharp": "2.6.11",
    "gatsby-plugin-twitter": "^2.3.1",
    "gatsby-plugin-typography": "^2.5.4",
    "gatsby-remark-autolink-headers": "^2.2.1",
    "gatsby-remark-copy-linked-files": "^2.2.1",
    "gatsby-remark-embed-gist": "^1.1.9",
    "gatsby-remark-embedder": "^2.0.1",
    "gatsby-remark-images": "^3.3.10",
    "gatsby-remark-sectionize": "^1.0.0",
    "gatsby-transformer-sharp": "2.4.3",
    "mousetrap": "^1.6.5",
    "plyr": "3.6.2",
    "prism-react-renderer": "^1.1.1",
    "prismjs": "^1.20.0",
    "react-anchor-link-smooth-scroll": "^1.0.11",
    "react-error-boundary": "^1.2.5",
    "react-helmet": "5.2.1",
    "react-intersection-observer": "^8.30.3",
    "react-live": "^2.2.2",
    "react-scrollspy": "^3.4.2",
    "react-simple-code-editor": "^0.11.0",
    "react-tooltip": "^4.2.5",
    "recharts": "^1.8.5",
    "remark-toc": "^7.0.0",
    "typeface-fira-code": "^1.1.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "7.9.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.5",
    "@testing-library/react": "^10.0.1",
    "@types/mousetrap": "^1.6.3",
    "@types/reach__router": "^1.3.5",
    "@types/react-helmet": "^5.0.15",
    "@types/react-scrollspy": "^3.3.2",
    "@types/recharts": "^1.8.12",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^2.31.0",
    "babel-jest": "^26.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react-hooks": "^4.0.0",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^2.0.0",
    "jest": "26.0.0",
    "jest-dom": "^3.0.2",
    "jest-emotion": "^10.0.32",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "semantic-release": "^17.0.7",
    "typescript": "^3.8.2"
  },
  "peerDependencies": {
    "@mdx-js/mdx": "^1.6.1",
    "@mdx-js/react": "^1.1.6",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "jest": {
    "snapshotSerializers": [
      "jest-emotion"
    ],
    "setupFilesAfterEnv": [
      "./setup-jest.js"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.[jt]sx?$": "<rootDir>/jest-preprocess.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleNameMapper": {
      "typeface-*": "identity-obj-proxy",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.js"
    },
    "collectCoverage": false,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  }
}
